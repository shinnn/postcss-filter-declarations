{
  "name": "postcss-filter-declarations",
  "version": "2.0.0",
  "description": "PostCSS plugin to filter declarations by property names",
  "repository": "shinnn/postcss-filter-declarations",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/postcss-filter-declarations/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "css",
    "style",
    "stylesheet",
    "postcss",
    "postcss-plugins",
    "declaration",
    "property",
    "remove",
    "include",
    "exclude",
    "filter"
  ],
  "devDependencies": {
    "eslint": "^0.15.1",
    "istanbul": "^0.3.6",
    "istanbul-coveralls": "^1.0.2",
    "jscs": "^1.11.3",
    "postcss": "^4.0.6",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
